                if j < 1
                    j = j + obj.n;
                end
                k = obj.iL - i;
                if k < 1
                    k = k+ obj.n;
                end
                if j > 3 && k >3
                    return
                end
                break
            end
            if j == 1
                obj.index = obj.iR;
                b = true;
                return
            end
            if k == 1
                obj.index = obj.iL;
                b = true;
                return
            end
            
            p = obj.ptD(z,i).leftStep;
            p2 = obj.ptD(z,i).rightStep;
            if j > k && p > 3
                if k == 1 || (p-k >=obj.re(obj.iL).leftStep && obj.re(obj.iL).rightStep - k <= obj.ptD(z,i).rightStep)
                    obj.index = obj.iL;
                else
                    return
                end
            elseif j<=3 && p2 > 3
                if k == 1 || (p2-j >=obj.re(obj.iL).rightStep && obj.re(obj.iL).leftStep - j <= obj.ptD(z,i).leftStep)
                    obj.index = obj.iL;
                else
                    return
                end
            else
                return
            end
            b = true;
            return
        end
        
